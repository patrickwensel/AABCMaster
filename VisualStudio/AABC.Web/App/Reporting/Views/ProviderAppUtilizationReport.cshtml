@using System.Web.Optimization
<div class="report-selector-container">

    <h1>Reports: Provider App Utilization Report</h1>

    <div class="new-layout">
        <div data-bind="datepickerRange:{}, datepickerOptions: { format: 'yyyy-mm-dd' }">
            <div class="form-group">
                <label for="startDate" class="col-sm-3 control-label"><span class="pull-left">Start Date</span></label>
                <div class="col-sm-9">
                    <input id="startDate" type="text" class="form-control date-range" data-bind="datepicker: startDate">
                </div>
            </div>
            <div class="form-group">
                <label for="endDate" class="col-sm-3 control-label"><span class="pull-left">End Date</span></label>
                <div class="col-sm-9">

                    <input id="endDate" type="text" class="form-control date-range" data-bind="datepicker: endDate">
                </div>
            </div>
        </div>

        <div class="clearfix"></div>
        <hr />

        <div id="chart"></div>

        <div class="clearfix"></div>
    </div>
</div>

@Scripts.Render("~/bundles/highcharts")
<script src="~/Scripts/Pages/ProviderAppUtilizationReport.js"></script>

<script type="text/javascript">
    $(document).ready(function() {
        App.NavBar.InitView(@Html.Raw(Json.Encode(ViewBag.Push)));
    });
</script>
